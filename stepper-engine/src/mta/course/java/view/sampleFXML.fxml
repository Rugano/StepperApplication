<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mta.course.java.controller.Controller">
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <top>
      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="LoadFileButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#clickMeButtonActionListener" prefHeight="31.0" prefWidth="120.0" text="Load File" />
            <TextField fx:id="XMLFilePathTextField" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="31.0" prefWidth="850.0" promptText="File Path" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </top>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <HBox fillHeight="false" prefHeight="100.0" prefWidth="200.0" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="FlowDefinitionButtonsVBox" spacing="10.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets></VBox>
                  <VBox fx:id="SelectedFlowDetailsVBox" prefHeight="200.0" prefWidth="100.0" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets top="25.0" />
               </padding>
            </HBox>
         </center>
      </BorderPane>
   </center>
</BorderPane>
